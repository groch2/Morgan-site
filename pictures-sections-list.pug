doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    style(type='text/css').
      ul {
        padding-left: 0
      }
  body
    ul(style="list-style-type:none;")
      each section in sections
        li= section.name
          ul(style="list-style-type:none;")
            each picture in section.pictures
              li
                | &nbsp;&nbsp;#{picture}
