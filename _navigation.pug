nav(id="left-side-nav-links")
  h1= "Morgan BANCON"
  each link, index in navLinks
    a(href=link.href style=index === contactLinkPosition ? 'text-decoration: underline' : '')= link.text
header
  h1= "Morgan BANCON"
  div(id="header-links")
    each link, index in navLinks
      a(href=link.href style=index === contactLinkPosition ? 'text-decoration: underline' : '')= link.text
  div(id="burger-menu-container")
    button(id="burger-menu" class="hamburger-box")
      div(class="hamburger-top")
      div(class="hamburger-middle")
      div(class="hamburger-bottom")
